
<html>
	<body>
		<form name="" action="/search">
			<input type="text" name="artist" placeholder="Artist"></input>
			<input type="text" name="query" placeholder="lyric"></input>
			<button type="submit">Search</button>
		</form>
		<button onclick="window.location='/add'">Add</button>
		<ul>
		{{#songs}}
		  <li>{{id}} - {{getSource.name}} - {{getSource.artist}} - {{getSource.year}} - {{getSource.album}} - {{getSource.lyrics}} 
		    {{#getHighlightFields}} - 
		      {{#lyrics.getFragments}}
		        {{#.}}{{{.}}}{{/.}}
		      {{/lyrics.getFragments}}
		    {{/getHighlightFields}}
		  </li>
		{{/songs}}
		</ul>
	</body>
</html>